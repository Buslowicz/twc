{
  "exclude": "**/node_modules/**/*",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-console": false,
    "no-string-literal": false,
    "no-namespace": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-empty-interface": false,
    "array-type": false
  }
}
