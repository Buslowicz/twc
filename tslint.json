{
  "exclude": "**/node_modules/**/*",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-console": false,
    "no-string-literal": false,
    "no-namespace": false,
    "quotemark": [
      true,
      "double"
    ],
    "no-empty-interface": false,
    "array-type": false,
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      140
    ]
  }
}
